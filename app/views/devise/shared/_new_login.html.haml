#log_in_container
  %h1 Login
  = form_for(resource, as: resource_name, url: session_path(resource_name), id: "login_form_main", :html => {:class => "login_form_main"}) do |f|
    / %form.form-inline{:role => "form"}
    .form-group#formgroup1
      / %label.sr-only
      = f.email_field :email, autofocus: true, :placeholder => "Email", id: "email_login_field"
    .form-group
      = f.password_field :password, autocomplete: "off", :placeholder => "Password", id: "password_login_field"
      = f.submit "Log in"
      %br
      = render "devise/shared/links"